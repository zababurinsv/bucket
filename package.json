{
  "name": "@orbit/bucket",
  "version": "1.0.0",
  "description": "task manager for async events",
  "source": "./src/index.mjs",
  "module": "./dist/index.mjs",
  "author": "Zababurin Sergey",
  "license": "GPL-3.0-only",
  "private": true,
  "config": {
    "port": 8449,
    "root": "src"
  },
  "scripts": {
    "start": "start",
    "dev": "parcel ./$npm_package_config_root/index.html -p $npm_package_config_port --cache-dir ./.parcel-cache",
    "build": "parcel build",
    "build:html": "parcel build src/index.html --dist-dir ./docs",
    "kill": "kill $(lsof -t -i:$npm_package_config_port)",
    "clean": "rimraf ./.parcel-cache"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.0.0-rc.0",
    "npm-run-all": "^4.1.5",
    "parcel": "2.0.0-rc.0",
    "parcel-reporter-static-files-copy": "^1.3.0"
  },
  "staticFiles": {
    "staticPath": "public"
  },
  "dependencies": {
    "z-events": "^0.2.4"
  }
}
